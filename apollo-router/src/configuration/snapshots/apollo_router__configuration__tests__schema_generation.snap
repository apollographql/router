    "ActiveRequestsAttributes": {
      "additionalProperties": false,
      "properties": {
        "http.request.method": {
          "default": false,
          "description": "The HTTP request method",
          "type": "boolean"
        },
        "server.address": {
          "default": false,
          "description": "The server address",
          "type": "boolean"
        },
        "server.port": {
          "default": false,
          "description": "The server port",
