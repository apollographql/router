          "properties": {
            "eq": {
              "items": {
                "$ref": "#/definitions/ConnectorSelectorOrValue"
              },
              "maxItems": 2,
              "minItems": 2,
              "type": "array"
            }
          },
          "required": [
            "eq"
          ],
          "type": "object"
        },
        {
          "additionalProperties": false,
          "description": "The first selection must be greater than the second selection.",
          "properties": {
            "gt": {
              "items": {
                "$ref": "#/definitions/ConnectorSelectorOrValue"
              },
              "maxItems": 2,
