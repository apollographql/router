        },
        "service_name": {
          "description": "The service you're trying to access, eg: \"s3\", \"vpc-lattice-svcs\", etc.",
          "type": "string"
        }
      },
      "required": [
        "access_key_id",
        "secret_access_key",
        "region",
        "service_name"
      ],
      "type": "object"
